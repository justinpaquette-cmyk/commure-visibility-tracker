        <key>Weekday</key>
        <integer>5</integer>
        <key>Hour</key>
        <integer>9</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>

    <key>StandardOutPath</key>
    <string>/Users/justinpaquette/Documents/sales eng projects v2/productivity/commure-task-tracker/data/recaps/wins-cron.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/justinpaquette/Documents/sales eng projects v2/productivity/commure-task-tracker/data/recaps/wins-cron.log</string>
</dict>
</plist>
