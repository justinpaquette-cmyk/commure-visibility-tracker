<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.commure.weekly-wins</string>

    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>/Users/justinpaquette/Documents/sales eng projects v2/productivity/commure-task-tracker/scripts/friday-wins.sh</string>
    </array>

    <!-- Run at 9 AM every Friday (weekday 5) -->
    <key>StartCalendarInterval</key>
